{
  "$schema": "appsettings-schema.json",
  "ConnectionStrings": {
    "umbracoDbDSN": "from-aspire-or-secrets",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "WithProcessId",
      "WithThreadId",
      "FromLogContext",
      "WithClientIp",
      "WithExceptionDetails"
    ]
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "DefaultUILanguage": "en-GB",
        "Id": "d378f676-9697-4e15-9cf1-e6bebc280298",
        "SanitizeTinyMce": true
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true,
        "LocalTempStorageLocation": "EnvironmentTemp"
      },
      //#if (ContentDeliveryApi)
      "DeliveryApi": {
        "Enabled": true,
        "RichTextOutputAsJson": true
      },
      //#endif
      "Unattended": {
        "UpgradeUnattended": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnFirstBoot": true,
      "ExportOnSave": "None",
      "UiEnabledGroups": "Settings"
    }
  }
}